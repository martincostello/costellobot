{
  "AllowedHosts": "*",
  "GitHub": {
    "AppId": "",
    "EnterpriseDomain": "",
    "InstallationId": 0,
    "PrivateKey": "",
    "WebhookSecret": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.DataProtection.KeyManagement.XmlKeyManager": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.EphemeralXmlRepository": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.FileSystemXmlRepository": "Error",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "Webhook": {
    "Approve": false,
    "ApproveComment": "Auto-approving dependency update.",
    "Automerge": false,
    "RerunFailedChecks": [
      "^macos-latest$",
      "^ubuntu-latest$",
      "^windows-latest$"
    ],
    "RerunFailedChecksAttempts": 0,
    "TrustedEntities": {
      "Dependencies": [
        "^Microsoft.AspNetCore\\..*$",
        "^Octokit$",
        "^Octokit.GraphQL$",
        "^Octokit.Webhooks.AspNetCore$",
        "^NodaTime$",
        "^NodaTime.Testing$"
      ],
      "Users": [
        "dependabot[bot]",
        "github-actions[bot]"
      ]
    }
  }
}
