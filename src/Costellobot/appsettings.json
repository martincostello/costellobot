{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AzureBlobStorage": "",
    "AzureKeyVault": "",
    "AzureServiceBus": ""
  },
  "Aspire": {
    "Azure": {
      "Messaging": {
        "ServiceBus": {
          "HealthCheckQueueName": "",
          "ClientOptions": {
            "Identifier": "costellobot"
          }
        }
      },
      "Storage": {
        "Blobs": {
          "ClientOptions": {
            "Diagnostics": {
              "ApplicationId": "costellobot"
            }
          }
        }
      }
    }
  },
  "GitHub": {
    "AccessToken": "",
    "AppId": "",
    "BadgesKey": "",
    "ClientId": "",
    "ClientSecret": "",
    "EnterpriseDomain": "",
    "InstallationId": 0,
    "PrivateKey": "",
    "Scopes": [],
    "WebhookSecret": ""
  },
  "HostOptions": {
    "ShutdownTimeout": "00:00:10"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.DataProtection.KeyManagement.XmlKeyManager": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.EphemeralXmlRepository": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.FileSystemXmlRepository": "Error",
      "Microsoft.Hosting.Lifetime": "Information",
      "Polly": "Warning",
      "System": "Warning"
    }
  },
  "Site": {
    "AdminRoles": [],
    "AdminUsers": []
  },
  "Webhook": {
    "Approve": false,
    "ApproveComment": "Auto-approving dependency update.",
    "ApproveLabels": [ "dependencies", "merge-approved" ],
    "Automerge": false,
    "AutomergeLabels": [ "dependencies", "merge-approved" ],
    "Deploy": false,
    "DeployComment": "Auto-approving deployment.",
    "DeployEnvironments": [],
    "IgnoreRepositories": [
      "martincostello/dotnet-patch-automation-sample"
    ],
    "PublishTimeout": "00:00:05",
    "QueueName": "webhooks",
    "Registries": {
      "Npm": {
        "BaseAddress": "https://registry.npmjs.org"
      },
      "NuGet": {
        "BaseAddress": "https://api.nuget.org"
      }
    },
    "RerunFailedChecks": [
      "^code-ql(\\s\\(csharp\\))?$",
      "^lighthouse$",
      "^macos-latest$",
      "^ubuntu-latest$",
      "^windows-latest$"
    ],
    "RerunFailedChecksAttempts": 0,
    "TrustedEntities": {
      "Dependencies": [
        "^@actions\/.*$",
        "^@microsoft\/signalr$",
        "^@typescript-eslint\/eslint-plugin$",
        "^@typescript-eslint\/parser$",
        "^AspNet.Security.OAuth\\..*$",
        "^BenchmarkDotNet$",
        "^eslint$",
        "^Microsoft.AspNetCore\\..*$",
        "^Microsoft.EntityFrameworkCore\\..*$",
        "^Microsoft.Extensions\\..*$",
        "^Microsoft.NET.Sdk$",
        "^Microsoft.NET.Test.Sdk$",
        "^NodaTime$",
        "^NodaTime.Testing$"
      ],
      "Publishers": {
        "GitHubActions": [
          "actions",
          "aws-actions",
          "azure",
          "github",
          "martincostello",
          "microsoft"
        ],
        "Npm": [
          "microsoft1es",
          "octokitbot",
          "types",
          "typescript-bot"
        ],
        "NuGet": [
          "aspire",
          "aspnet",
          "awsdotnet",
          "azure-sdk",
          "dotnetfoundation",
          "dotnetframework",
          "EntityFramework",
          "GitHub",
          "JUSTEAT_OSS",
          "martin_costello",
          "Microsoft",
          "NodaTime",
          "Polly"
        ],
        "Submodules": [
          "https://github.com/martincostello"
        ]
      },
      "Users": [
        "costellobot",
        "costellobot[bot]",
        "dependabot[bot]",
        "github-actions[bot]"
      ]
    }
  }
}
